using namespace std;

int intersect(int n, int f, int m) {

	int diff = f - m;

	if(diff < 0) {
		diff = -diff;
	}

	if(n - diff < diff) {
		diff = n - diff;
	}

	if(diff < 5) {

	}

	return 0;
}

int main() {

	ifstream fin("combo.in");
	ofstream fout("combo.out");

