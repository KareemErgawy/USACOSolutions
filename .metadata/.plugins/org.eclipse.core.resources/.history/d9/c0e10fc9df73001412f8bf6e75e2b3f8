#include <iostream>
#include <fstream>
#include <cstring>

using namespace std;

int n;
int* pairings;

void discoverPairings(int level) {
	if (level == n) {
		return;
	}

	int h1 = -1;
	for (int i = 0; i < n; i++) {
		if (pairings[i] == -1) {
			if(h1 == -1){
				h1 = i;
			} else {
				pairings[h1] = i;
				pairings[i] = h1;
				discoverPairings(level+1);
			}
		}
	}
}

int main() {

	ifstream fin("wormhole.in");
	ofstream fout("wormhole.out");

	fin >> n;
	int** holePositions = new int*[n];

	for (int i = 0; i < n; i++) {
		holePositions[i] = new int[2];
		fin >> holePositions[i][0] >> holePositions[i][1];
	}

	pairings = new int[n];
	memset(pairings, -1, sizeof(int) * n);

	return 0;
}
