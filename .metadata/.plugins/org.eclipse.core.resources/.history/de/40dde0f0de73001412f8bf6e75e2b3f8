#include <iostream>
#include <fstream>
#include <cstring>

using namespace std;

int* pairings;

int main() {

	ifstream fin("wormhole.in");
	ofstream fout("wormhole.out");

	int n;
	fin >> n;
	int** holePositions = new int*[n];

	for (int i = 0; i < n; i++) {
		holePositions[i] = new int[2];
		fin >> holePositions[i][0] >> holePositions[i][1];
	}

	pairings = new int[n];
	memset(pairings, 0, sizeof(int) * n);

	return 0;
}
