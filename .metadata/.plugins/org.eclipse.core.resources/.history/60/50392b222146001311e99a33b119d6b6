#include <iostream>
#include <fstream>
#include <vector>

using namespace std;

vector<string> dict;

bool searchInDict(int low, int hi, string target) {
	if (low > hi) {
		return false;
	}

	int mid = (hi - low) / 2;
	if (target.compare(dict[mid]) == 0) {
		return true;
	} else if (target > dict[mid]) {
		return searchInDict(mid + 1, hi, target);
	} else {
		return searchInDict(low, mid - 1, target);
	}
}

void printAllCombinations(string n, char charNumMapping[10][3],
		string prefix = "", int level = 0) {
	if (level == n.length()) {
		//cout << prefix << endl;

		if(prefix.compare("GREG") == 0){
			cout << "dummy" << endl;
		}

		//if (searchInDict(0, dict.size(), prefix)) {
			cout << prefix << endl;
		//}
		return;
	}

	for (int i = 0; i < 3; i++) {
		printAllCombinations(n, charNumMapping,
				prefix + charNumMapping[n[level] - '0'][i], level + 1);
	}
}

int main() {

	ifstream fin("namenum.in");
	ofstream fout("namenum.out");
	ifstream dictin("dict.txt");

	while (dictin.good()) {
		string word;
		dictin >> word;
		dict.push_back(word);
	}

	searchInDict(0, dict.size(), "GREG");

	char charNumMapping[][3] = { { ' ', ' ', ' ' }, { ' ', ' ', ' ' }, { 'A',
			'B', 'C' }, { 'D', 'E', 'F' }, { 'G', 'H', 'I' },
			{ 'J', 'K', 'L' }, { 'M', 'N', 'O' }, { 'P', 'R', 'S' }, { 'T',
					'U', 'V' }, { 'W', 'X', 'Y' } };

	cout << dict.size() << endl;

	for (int i = 2; i < 10; i++) {

		for (int j = 0; j < 3; j++) {
			//			charNumMapping[i][j] = 'A' + ((i - 2) * 3) + j;
			cout << charNumMapping[i][j] << " ";
		}

		cout << endl;
	}

	printAllCombinations("4734", charNumMapping);

	return 0;
}
