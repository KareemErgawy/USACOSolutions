#include <iostream>
#include <fstream>
using namespace std;

void generateFraction(int n, int n1, int d1, int n2, int d2) {
	if (n1 > d1 || n2 > d2) {
		return;
	}

	if (d1 + d2 > n) {
		return;
	}

	generateFraction(n, n1, d1, n1 + n2, d1 + d2);

	generateFraction(n, n1 + n2, d1 + d2, n2, d2);
}
