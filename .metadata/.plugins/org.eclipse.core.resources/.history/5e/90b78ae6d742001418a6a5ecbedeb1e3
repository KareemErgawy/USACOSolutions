#include <iostream>
#include <fstream>
using namespace std;

string encoding[] = { "", "I", "II", "III", "IV", "V", "VI", "VII", "VIII",
		"IX" };

string romanDigit(int digit, string map) {

	string encodingMap = encoding[digit % 10];
	encodingMap.replace(encodingMap.begin(), encodingMap.end(), 'I', map[0]);
	encodingMap.replace(encodingMap.begin(), encodingMap.end(), 'V', map[1]);
	encodingMap.replace(encodingMap.begin(), encodingMap.end(), 'X', map[2]);

	return encodingMap;
}

int main() {

	cout << romanDigit(5, "XLC") << endl;

	ifstream fin("preface.in");
	ofstream fout("preface.out");

	int n;
	fin >> n;

	return 0;
}
