#include <iostream>
#include <fstream>
using namespace std;

long gcd(long n, long m) {
	if (n < m) {
		long tmp = n;
		n = m;
		m = tmp;
	}

	if (n % m == 0) {
		return m;
	}

	return gcd(m, n % m);
}

long lcm(long n, long m) {
	return n / gcd(n, m) * m;
}

int main() {
	long res = 1;

	for (int i = 1; i <= 50; i++) {
		res = lcm(res, i);
	}

	cout << res << endl;

	return 0;
}
