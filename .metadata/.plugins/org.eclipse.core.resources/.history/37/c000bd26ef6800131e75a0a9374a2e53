#include <iostream>
#include <fstream>
using namespace std;

unsigned long long gcd(unsigned long long n, unsigned long long m) {
	if (n < m) {
		unsigned long long tmp = n;
		n = m;
		m = tmp;
	}

	if (n % m == 0) {
		return m;
	}

	return gcd(m, n % m);
}

unsigned long long lcm(unsigned long long n, unsigned long long m) {
	return n / gcd(n, m) * m;
}

int main() {
	unsigned long long res = 1;

	for (int i = 1; i <= 100; i++) {
		res = lcm(res, i);
	}

	cout << res << endl;

	return 0;
}
