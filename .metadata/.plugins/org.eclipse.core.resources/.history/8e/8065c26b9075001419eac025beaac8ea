#include <iostream>
#include <fstream>
#include <cstring>

using namespace std;

int main() {
	ifstream fin("wormhole.in");
	ofstream fout("wormhole.out");
	int n;
	fin >> n;
	int* x = new int[n + 1];
	int* y = new int[n + 1];
	int* right_neighbor = new int[n + 1];

	for (int i = 1; i <= n; i++) {
		fin >> x[i] >> y[i];
	}

	return 0;
}
