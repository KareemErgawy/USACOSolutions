#include <iostream>
#include <fstream>
using namespace std;

int n;
int maxVal;
int* solution;

int calculateDistance2(int x, int y) {
	int xOr = x ^ y;
	int distance = 0;
	while (xOr != 0) {
		xOr = (xOr ^ (xOr & -xOr));
		distance++;
	}
	return distance;
}

void search(int index, int start) {
	if (index == n) {
		return;
	}

	for(int i=start ; i<maxVal ; i++){
		for(int j=0 ; j<index ; j++){

		}
	}
}

int main() {
	int b, d;
	ifstream fin("hamming.in");
	ofstream fout("hamming.out");

	fin >> n >> b >> d;
	maxVal = 2 << d;
	int** distances = new int*[maxVal];

	for (int i = 0; i < maxVal; i++) {
		distance[i] = new int[maxVal];
		for (int j = i + 1; j < maxVal; j++) {
			distance[i][j] = calculateDistance2(i, j);
		}
	}

	solution = new int[n];
	solution[0] = 0;

	return 0;
}
