#include <iostream>
#include <fstream>
using namespace std;

void processRoman(int n) {
	int nM = 0;
	int nD = 0;
	int nC = 0;
	int nL = 0;
	int nX = 0;
	int nV = 0;
	int nI = 0;

	while(n >= 1000){
		n -= 1000;
		nM++;
	}

	while(n >= 500){
		n -= 500;
		nD++;
	}

	while(n >= 100){
		n -= 100;
		nC++;
	}

	while(n >= 50){
		n -= 50;
		nL++;
	}

	while(n >= 10){
		n -= 10;
		nX++;
	}

	while(n >= 5){
		n -= 5;
		nV++;
	}

	while(n >= 1){
		n -= 1;
		nI++;
	}
}

int main() {
	return 0;
}
