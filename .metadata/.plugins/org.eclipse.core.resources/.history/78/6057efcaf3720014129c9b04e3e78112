#include <iostream>
#include <fstream>
#include <cmath>

using namespace std;

// Calculates the number of intersecting values between f and m
// For example, if n = 50, f = 6, and m = 6 the function returns 5
// Another example, if n = 50, f = 6, and m = 4 the function returns 2
int intersect(int n, int f, int m) {
	if (n < 5) {
		return n;
	} else if (diff < 5) {
		int diff = f - m;

		if (diff < 0) {
			diff = -diff;
		}

		if (n - diff < diff) {
			diff = n - diff;
		}
		return 5 - diff;
	}

	return 0;
}

int main() {

	ifstream fin("combo.in");
	ofstream fout("combo.out");

	int n;
	int f[3];
	int m[3];

	fin >> n;
	fin >> f[0] >> f[1] >> f[2];
	fin >> m[0] >> m[1] >> m[2];
	int total = 2 * pow(min(5, n), 3);

	fout << (total - (intersect(n, f[0], m[0]) * intersect(n, f[1], m[1])
			* intersect(n, f[2], m[2]))) << endl;

	return 0;
}
