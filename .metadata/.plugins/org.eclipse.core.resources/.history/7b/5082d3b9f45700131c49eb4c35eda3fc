#include <iostream>
using namespace std;

int* bisquares;

void generateBisquares(int m) {
	bisquares = new int[m * m * 2 + 1];

	for (int i = 0; i < m * m * 2 + 1; i++) {
		bisquares[i] = false;
	}

	for (int i = 0; i <= m; i++) {
		for (int j = 0; j <= m; j++) {
			bisquares[(i * i) + (j * j)] = true;
		}
	}

	for (int i = 0; i < m * m * 2 + 1; i++) {
		if (bisquares[i])
			cout << i << endl;
	}
}

int main() {
	generateBisquares(7);

	return 0;
}
