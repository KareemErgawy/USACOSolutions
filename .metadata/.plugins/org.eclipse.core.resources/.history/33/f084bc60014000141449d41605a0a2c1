#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int convert(double x) {
	return round((pow(x / 255, 1 / 2.2) * 255));
}

int main(int argc, char **argv) {
	double x = 1;
	x = convert(x);

	char *buf;

	buf = (char*)malloc(1 << 31);

	fgets(buf, 1024, stdin);
	printf("%s\n", buf);

	return 1;
}
