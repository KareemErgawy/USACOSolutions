#include <iostream>
#include <fstream>
#include <cmath>

using namespace std;

int intersect(int n, int f, int m) {

	int diff = f - m;

	if (diff < 0) {
		diff = -diff;
	}

	if (n - diff < diff) {
		diff = n - diff;
	}

//	int comp = min(5, n);
	if (n < 5) {
		return n;
	} else if (diff < 5) {
		return 5 - diff;
	}

	return 0;
}

int main() {

	ifstream fin("combo.in");
	ofstream fout("combo.out");

	int n;
	int f[3];
	int m[3];

	fin >> n;
	fin >> f[0] >> f[1] >> f[2];
	fin >> m[0] >> m[1] >> m[2];
	int total = 2 * pow(min(5, n), 3);

	fout << (total - (intersect(n, f[0], m[0]) * intersect(n, f[1], m[1])
			* intersect(n, f[2], m[2]))) << endl;

	return 0;
}
